<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitchView">
        <attr name="search_hint_sv" format="string" />
        <attr name="spinner_type" format="enum">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
        </attr>
        <attr name="defText" format="string" />
    </declare-styleable>

    <declare-styleable name="MultiSpinner">
        <attr name="search_hint" format="string" />
        <attr name="multi_spinner_type" format="enum">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomFont">
        <attr name="fontName" format="string" />
        <attr name="fontStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="Thin" value="1" />
            <enum name="Light" value="2" />
            <enum name="Regular" value="3" />
            <enum name="Icon" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AddMembersCustomView">
        <attr name="pageScroller" format="reference" />
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>


    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoPanelHeightMin" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParalaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoOverlay" format="boolean" />
        <attr name="umanoClipPanel" format="boolean" />
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="withoutPadding" format="boolean" />
    </declare-styleable>

</resources>